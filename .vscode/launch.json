  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Server",
      "type": "python",
      "request": "launch",
      "program": "_server.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Client",
      "type": "python",
      "request": "launch",
      "program": "_client.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Real Client",
      "type": "python",
      "request": "launch",
      "program": "client.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Attach using Process Id",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
